   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "colab": {
     "base_uri": "https://localhost:8080/"
    },
    "id": "jvdXuMzbJ44d",
    "outputId": "8c35ee3b-9dbd-4a2e-c11a-857fa688d535"
   },
   "outputs": [],
   "source": [
