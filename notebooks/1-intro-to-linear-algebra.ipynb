      "cell_type": "code",
      "metadata": {
        "id": "e_zDOxgHiezz",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "7f31d30b-fc5d-46b7-e0d9-139fb25ec4dc"
      },
      "source": [
        "d_r = 2.5 * t\n",
